package main

import "fmt"

func main() {
	var x int
	var y int
	fmt.Print("Insira o primeiro número inteiro: ")
	fmt.Scan(&x)
	fmt.Print("Insira o segundo número inteiro: ")
	fmt.Scan(&y)
	var resultado int
	if x > 0 && y > 0 {
		resultado = x * y
	} else {
		resultado = x + y
	}
	fmt.Print("O resultado é ", resultado)
}

